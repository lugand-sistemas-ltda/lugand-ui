{
  "name": "@lugand-sistemas-ltda/vue-ui-lib",
  "version": "0.1.7",
  "description": "Sistema robusto de componentes genéricos e reutilizáveis Vue 3 + TypeScript",
  "private": false,
  "type": "module",
  "main": "./dist/vue-ui-lib.js",
  "module": "./dist/vue-ui-lib.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/vue-ui-lib.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./styles": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui",
    "design-system",
    "typescript",
    "vite",
    "hsl"
  ],
  "author": "Lugand Sistemas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lugand-sistemas-ltda/lugand-ui.git",
    "directory": "packages/ui-lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "sass": "^1.97.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "esbuild": "^0.25.12",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4",
    "vue-tsc": "^3.1.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}